<?xml version="1.0"?>
<jaxb:bindings version="1.0"
              xmlns:jaxb="http://java.sun.com/xml/ns/jaxb"
              xmlns:xsd="http://www.w3.org/2001/XMLSchema">
    <jaxb:globalBindings>
        <jaxb:serializable/>
        <jaxb:javaType name="java.time.LocalDate" xmlType="xs:date"
                       parseMethod="fith.SchoolJaxbUtils.fromXML"
                       printMethod="fith.SchoolJaxbUtils.toXML"/>
    </jaxb:globalBindings>
</jaxb:bindings>

